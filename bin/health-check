#!/usr/bin/env python3

import urllib.request

try:
    response = urllib.request.urlopen('http://localhost:5000/api/health-check')
    if response.getcode() == 200:
        print("Flask server is running")
        exit(0)
    else:
        print("Flask server is not running")
        exit(1)
except Exception as e:
  print(f"Error: {e}")
  exit(1)
